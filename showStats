using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace consoleNew
{
    class Program
    {
        static void Main(string[] args)
        {
            Player player1 = new Player(300,50,25,100);
            player1.ShowStats();
        }
    }

    class Player
    {
        public int Health;
        public int Armor;
        public int Damage;
        public int Speed;

        public Player(int health, int armor, int damage, int speed)
        {
            Health = health;
            Armor = armor;
            Damage = damage;
            Speed = speed;
        }

        public void ShowStats()
        {
            Console.WriteLine(Armor + " " + Health + " " +  Damage + " " + Speed);
        }
    }
}
